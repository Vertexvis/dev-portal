{
  "name": "dev-portal",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "docusaurus build",
    "clean": "rm -rf node_modules .docusaurus",
    "deploy": "docusaurus deploy",
    "format": "prettier --write './**/*.+(js|jsx|ts|tsx|json|yml|yaml|md|mdx|html|css)'",
    "lint": "eslint . --ext .js --config .eslintrc",
    "lint-staged": "yarn lint && yarn format",
    "postinstall": "husky install",
    "prepare": "husky install",
    "start": "docusaurus start",
    "version-docs": "docusaurus docs:version",
    "swizzle": "docusaurus swizzle"
  },
  "dependencies": {
    "@algolia/client-search": "^4.9",
    "@docusaurus/core": "^2.0.0-alpha.73",
    "@docusaurus/plugin-client-redirects": "^2.0.0-alpha.73",
    "@docusaurus/preset-classic": "^2.0.0-alpha.73",
    "classnames": "^2.3",
    "docusaurus-gtm-plugin": "^0.0.2",
    "mermaid": "^8",
    "react": "^17.0",
    "react-dom": "^17.0",
    "react-hubspot-form": "^1.3.7",
    "swagger-ui": "^3",
    "swagger-ui-react": "^3"
  },
  "devDependencies": {
    "@types/react": "^17.0",
    "@vertexvis/eslint-config-vertexvis": "^0.6",
    "eslint": "^7.25",
    "eslint-plugin-react": "^7.23",
    "husky": "^6.0",
    "prettier": "^2.2",
    "raw-loader": "^4.0"
  },
  "workspaces": {
    "nohoist": [
      "**/html-minifier-terser"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
